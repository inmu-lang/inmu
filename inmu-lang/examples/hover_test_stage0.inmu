# ホバー機能テスト (Stage 0)

/**
 * ユーザーの年齢を保存する変数
 * 18歳以上かどうかをチェックするのに使用
 */
let age = 20

/**
 * ユーザーの名前
 */
let name = "Taro"

# JSDocコメントなしの変数
let counter = 0

# 組み込み関数のテスト
print "Hello, World!"
print age
print name

# アサーション
assert(age, 20)
assert_ne(counter, -1)

# 条件分岐
if age == 18
  print "18歳です"
else
  print "18歳ではありません"
endif

print "完了！"
